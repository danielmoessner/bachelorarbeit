@InProceedings{MYRS05,
  author    = {R.~Manevich and E.~Yahav and G.~Ramalingam and M.~Sagiv},
  title     = {Predicate Abstraction and Canonical Abstraction for Singly-Linked Lists},
  booktitle = {Proc.\ VMCAI},
  year      = {2005},
  series    = {LNCS~3385},
  pages     = {181-198},
  publisher = {Springer},
  annot     = { The paper is a case study comparing canonical and predicate abstraction for the analysis of singly-linked list program. There is that a predicate abstraction equivalent to any finatary abstraction (including canonical abstraction), but it might contain exponentially more predicates. The paper shows a case where the predicate abstraction has sub-exponentially more predicates (quadratic) than an equivalent canonical abstraction. In addition, there is claim that state-of-the-art iterative refinement techniques fail to prove interesting properties about list, which is the case for \blast{} with predicate abstraction only. },
}

@InProceedings{DN03,
  author    = {D.~Dams and K.~S.~Namjoshi},
  title     = {Shape Analysis through Predicate Abstraction and Model Checking},
  booktitle = {Proc.\ VMCAI},
  year      = {2003},
  series    = {LNCS~2575},
  pages     = {310-324},
  publisher = {Springer},
  annot     = { The paper presents a way of designing a predicate abstraction for performing shape analysis based on predicates. Given a program, they try to find a suitable predicate abstraction by computing weakest preconditions, and given abstraction hints given by the user. Because of this hints, the method is not fully automatized since the user has to discover the approximations needed for proving the program safe (e.g., the user decide to add the cyclicity predicate at some point). The generated abstract program is then model check with SPIN or COSPAN. The only example given in the paper is a list reversal program. },
}

@InProceedings{BPZ05,
  author    = {I.~Balaban and A.~Pnueli and L.~D.~Zuck},
  title     = {Shape Analysis by Predicate Abstraction},
  booktitle = {Proc.\ VMCAI},
  year      = {2005},
  series    = {LNCS~3385},
  pages     = {164-180},
  publisher = {Springer},
  annot     = { The paper presents an predicate-abstraction based approach to shape analysis. Instead of modeling the heap directly, they use predicates, typically reachability (of a variable from another). The verification is then done by BDD-based symbolic model checking. They focus on singly-linked list, and are able to prove safety and liveness properties of programs (reversal and bubble sort). No automatic refinement procedure is given. },
}

