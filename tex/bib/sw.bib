@InProceedings{GDDRS04,
  author    = {D.~Gopan and F.~DiMaio and N.~Dor and T.~W.~Reps and M.~Sagiv},
  title     = {Numeric Domains with Summarized Dimensions},
  booktitle = {Proc.\ TACAS},
  year      = {2004},
  series    = {LNCS~2988},
  pages     = {512-529},
  publisher = {Springer},
  annot     = { The paper presents a way of combining numeric domain (which may or may not be relational) with static analysis that use summarization (like shape analysis in \tvla). The contribution of the paper is to show how to build a summarizing numerical abstraction given a numeric domain and a {\em client} (such as \tvla). Array-based examples are given. The numeric properties are about the content of the elements, and not about the number of elements being summarized (like in Yavuz and Bultan, {\em Automated Verification of Concurrent Linked Lists with Counters}). It does not talk about refinement, nor about any link with predicate abstraction. },
}

